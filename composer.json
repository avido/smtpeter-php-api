{
    "name": "avido/smtpeter-php-api",
    "version": "0.0.3",
    "description": "Api client for smtpeter.com",
    "keywords": [
        "smtpeter",
        "php",
        "api"
    ],
    "homepage": "https://github.com/avido/smtpeter-php-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Blaas",
            "email": "sebastiaan@avido.nl"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/collections": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.5",
        "overtrue/phplint": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Avido\\Smtpeter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Avido\\Smtpeter\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "style": "vendor/bin/phpcs --standard=PSR2 --extensions=php --colors src/*",
        "lint": "vendor/bin/phplint --no-configuration --no-cache --no-interaction --extensions=php src/*"
    }
}
